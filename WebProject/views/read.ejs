<!--Write-->
<!DOCTYPE html>
<html lang="en">
	<style>
		table.type02 {
			border-collapse: separate;
			border-spacing: 0;
			text-align: center;
			line-height: 1.5;
			border-top: 1px solid #ccc;
			border-left: 1px solid #ccc;
			  width: 80%;
			 margin : auto;
		}
		table.type02 th {
			width: 150px;
			padding: 10px;
			font-weight: bold;
			text-align: center;
			vertical-align: top;
			border-right: 1px solid #ccc;
			border-bottom: 1px solid #ccc;
			border-top: 1px solid #fff;
			border-left: 1px solid #fff;
			background: #eee;
		}
		table.type02 td {
			width: 350px;
			padding: 10px;
			vertical-align: center;
			border-right: 1px solid #ccc;
			border-bottom: 1px solid #ccc;
		}
	</style>
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		 <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

		<title>Electro - HTML Ecommerce Template</title>

 		<!-- Google font -->
 		<link href="https://fonts.googleapis.com/css?family=Montserrat:400,500,700" rel="stylesheet">

 		<!-- Bootstrap -->
 		<link type="text/css" rel="stylesheet" href="/electro/css/bootstrap.min.css"/>

 		<!-- Slick -->
 		<link type="text/css" rel="stylesheet" href="/electro/css/slick.css"/>
 		<link type="text/css" rel="stylesheet" href="/electro/css/slick-theme.css"/>

 		<!-- nouislider -->
 		<link type="text/css" rel="stylesheet" href="/electro/css/nouislider.min.css"/>

 		<!-- Font Awesome Icon -->
 		<link rel="stylesheet" href="/electro/css/font-awesome.min.css">

 		<!-- Custom stlylesheet -->
 		<link type="text/css" rel="stylesheet" href="/electro/css/style.css"/>

		<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
		<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
		<!--[if lt IE 9]>
		  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
		  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
		<![endif]-->

    </head>
	<body>
		<!-- HEADER -->
		<header>
						<!-- TOP HEADER -->
                        <div id="top-header">
                            <div class="container">
                                <ul class="header-links pull-left">
                                    <li><div style="color:white">셀폰 - 중고 휴대폰 개인 거래</div></li>
                                </ul>
                                <ul class="header-links pull-right">
									<% if( user == null) { %>
										<li><a href="/sellphone/login">로그인</a></li>
									<% } else { %>
										<form action="/sellphone/logout" method="post">
											<li><button style="border:0; outline:0; background-color: #1E1F29;color:white;" type = "submit">로그아웃</button></li>
											<!-- <input type="">로그아웃 -->
										</form>
									<% } %>
									<li><a href="#">고객센터</a></li>
								</ul>
                            </div>
                        </div>
                        <!-- /TOP HEADER -->

			<!-- MAIN HEADER -->
			<div id="header">
				<!-- container -->
				<div class="container">
					<!-- row -->
					<div class="row">
						<!-- LOGO -->
						<div class="col-md-3">
							<div class="header-logo">
								<a href="/sellphone/home" class="logo">
									<img src="/electro/img/my_logo.png" alt="">
								</a>
							</div>
						</div>
						<!-- /LOGO -->

<!-- SEARCH BAR -->
<div class="col-md-6">
    <div class="header-search">
        <form>
            <select class="input-select">
                <option value="0">Sell Phone</option>
                <option value="1">Category 01</option>
                <option value="1">Category 02</option>
            </select>
            <input class="input" placeholder="HnB">
            <button class="search-btn">Search</button>
        </form>
    </div>
</div>
<!-- /SEARCH BAR -->

<!-- ACCOUNT -->
<div class="col-md-3 clearfix">
    <div class="header-ctn">
        <!-- Wishlist -->
        <div>
            <% if( user == null) { %>
				<a href="/sellphone/login">
					<i class="fa fa-pencil-square-o"></i>
					<span>상품 등록</span>
					<div class="qty">2</div>
				</a>
			<% } else { %>
				<a href="/sellphone/sell">
					<i class="fa fa-pencil-square-o"></i>
					<span>상품 등록</span>
					<div class="qty">2</div>
				</a>
			<% } %>
        </div>
        <!-- /Wishlist -->



        <!-- MYPAGE -->
        <div>
            <% if( user == null) { %>
				<a href="/sellphone/login" class="dropdown-toggle" aria-expanded="true">  <!-- data-toggle="dropdown"-->
					<i class="fa fa-user-o"></i>
					<span>마이페이지</span>
					<div class="qty"></div>
				</a>
			<% } else { %>
				<a href="/sellphone/mypage" class="dropdown-toggle" aria-expanded="true">  <!-- data-toggle="dropdown"-->
					<i class="fa fa-user-o"></i>
					<span>마이페이지</span>
					<div class="qty"><%=user[0].customer_name%></div>
				</a>
			<% } %>
        </div>
        <!-- /MYPAGE -->

								<!-- Menu Toogle -->
								<div class="menu-toggle">
									<a href="#">
										<i class="fa fa-bars"></i>
										<span>Menu</span>
									</a>
								</div>
								<!-- /Menu Toogle -->
							</div>
						</div>
						<!-- /ACCOUNT -->
					</div>
					<!-- row -->
				</div>
				<!-- container -->
			</div>
			<!-- /MAIN HEADER -->
		</header>
		<!-- /HEADER -->

		<!-- NAVIGATION -->
		<nav id="navigation">
			<!-- container -->
			<div class="container">
				<!-- responsive-nav -->
				<div id="responsive-nav">
					<!-- NAV -->
					<!-- /NAV -->
				</div>
				<!-- /responsive-nav -->
			</div>
			<!-- /container -->
		</nav>
		<!-- /NAVIGATION -->

		<!-- BREADCRUMB -->
		<div id="breadcrumb" class="section">
			<!-- container -->
			<div class="container">
				<!-- row -->
				<div class="row">
					<div class="col-md-12">
						<h3 class="breadcrumb-header">상품조회</h3>
						<ul class="breadcrumb-tree">
							<li><a href="/sellphone/home">Home</a></li>
							<li class="active">상품조회</li>
						</ul>
					</div>
				</div>
				<!-- /row -->
			</div>
			<!-- /container -->
		</div>
		<!-- /BREADCRUMB -->

		<!-- SECTION -->
		<div class="section">
			<!-- container -->
			<div class="container">
				<!-- row -->
				<div class="row">
					<!-- Product main img -->
					<div class="col-md-5 col-md-push-2">
						<div id="product-main-img">
							<div class="product-preview">
								<img src="/electro/img/<%=row.product_img1%>" alt="">
							</div>

							<div class="product-preview">
								<img src="/electro/img/<%=row.product_img2%>" alt="">
							</div>

							<div class="product-preview">
								<img src="/electro/img/<%=row.product_img3%>" alt="">
							</div>
						</div>
					</div>
					<!-- /Product main img -->

					<!-- Product thumb imgs -->
					<div class="col-md-2  col-md-pull-5">
						<div id="product-imgs">
							<div class="product-preview">
								<img src="/electro/img/<%=row.product_img1%>" alt="">
							</div>

							<div class="product-preview">
								<img src="/electro/img/<%=row.product_img2%>" alt="">
							</div>

							<div class="product-preview">
								<img src="/electro/img/<%=row.product_img3%>" alt="">
							</div>
						</div>
					</div>
					<!-- /Product thumb imgs -->

					<!-- Product details -->
					<div class="col-md-5">
						<div class="product-details">
							<h2 class="product-name"><%=row.product_title%><a style=" font-size:14px; color:#e4001b;">&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp<%=row.product_state%></a></h2>
							<div>
								<div class="product-rating">
									<!-- <i class="fa fa-star"></i>
									<i class="fa fa-star"></i>
									<i class="fa fa-star"></i>
									<i class="fa fa-star"></i>
									<i class="fa fa-star-o"></i> -->
									판매자 : <%=row.product_saler%><br>
								</div>
								<a class="review-link">상품등록일	: <%=row.product_date%><br></a>
								<!-- <a>조회수	: <%=row.product_hit%></a> -->
							</div>
							<div>
								<h3 class="product-price"><%=row.product_price * 0.9%> <del class="product-old-price"><%=row.product_price%></del></h3>
								<% var date = new Date().toISOString().substr(0, 10).replace('T', ' '); 
								if(date == row.product_date) { %>
									<span class="product-available">금일 신상</span>
								<% } else { %>
									<span class="product-available"></span>
								<% } %>
							</div>
							<p><%=row.product_description%></p>

							<div class="product-options">
								제조회사&nbsp&nbsp&nbsp&nbsp:&nbsp&nbsp&nbsp&nbsp<%=row.product_manufacturer%><br>
								기기상태&nbsp&nbsp&nbsp&nbsp:&nbsp&nbsp&nbsp&nbsp<%=row.product_status%><br>
								기기색상&nbsp&nbsp&nbsp&nbsp:&nbsp&nbsp&nbsp&nbsp<%=row.product_color%><br>
								내부용량&nbsp&nbsp&nbsp&nbsp:&nbsp&nbsp&nbsp&nbsp<%=row.product_storage%><br>
								거래방법&nbsp&nbsp&nbsp&nbsp:&nbsp&nbsp&nbsp&nbsp<%=row.product_trade_facetoface%>&nbsp&nbsp&nbsp<%=row.product_trade_point%><br>
								거래장소&nbsp&nbsp&nbsp&nbsp:&nbsp&nbsp&nbsp&nbsp<%=row.product_locate%><br>
								

							</div>

							<div class="add-to-cart">
								<% if(user == null) { %>
									<button class="add-to-cart-btn" onclick="reviewbtn()"><i class="fa fa-shopping-cart"></i> 구매하기</button>
								<% } else if(user!=null && deal[0].deal_seller == user[0].customer_id) { %>
									<button class="add-to-cart-btn"><i class="fa fa-shopping-cart"></i> 판매완료</button>
								<% } else if(deal[0].deal_buyer == '') { %>
									<form action="/payment/<%=row.product_id%>" method="get">
										<button class="add-to-cart-btn"><i class="fa fa-shopping-cart"></i> 구매하기</button>
									</form>
								<% } else { %>
									<button class="add-to-cart-btn" onclick="nobuy()"><i class="fa fa-shopping-cart"></i> 구매하기</button>
								<% } %>
							</div>

							<ul class="product-btns">
								<form action="/sellphone/interest/<%=row.product_id%>" method="post">
								<li><button type="submit" style="background-color: white; border: 0;" onclick="wishlist()"><i class="fa fa-heart-o"></i> 찜목록 추가</button></li>
								
								<li><i class="fa fa-user-circle-o"></i>  찜한 사람 : <%=row.product_interest%></li>
								<li><i class="fa fa-users"></i>  조회수 : <%=row.product_hit%></li>
								</form>
							</ul>

							<ul class="product-links">
								<li>Category:</li>
								<li><a href="#"><%=row.product_manufacturer%></a></li>
								<!-- <li><a href="#">Accessories</a></li> -->
							</ul>

							<ul class="product-links">
								<li>Share:</li>
								<li><a href="#"><i class="fa fa-facebook"></i></a></li>
								<li><a href="#"><i class="fa fa-twitter"></i></a></li>
								<li><a href="#"><i class="fa fa-google-plus"></i></a></li>
								<li><a href="#"><i class="fa fa-envelope"></i></a></li>
							</ul>

						</div>
					</div>
					<!-- /Product details -->

					<!-- Product tab -->
					<div class="col-md-12">
						<div id="product-tab">
							<!-- product tab nav -->
							<ul class="tab-nav">
								<li class="active"><a data-toggle="tab" href="#tab1">내 찜목록</a></li>
								<li><a data-toggle="tab" href="#tab2">Q&A</a></li>
								<li><a data-toggle="tab" href="#tab3">리뷰 (<%=review.length%>)</a></li>
							</ul>
							<!-- /product tab nav -->

							<!-- product tab content -->
							<div class="tab-content">
								<!-- tab1  -->
								<div id="tab1" class="tab-pane fade in active">
									<div class="row">
										<div class="col-md-12">
											<% if(user != null){ %>
											<table class="type02">
												<thead>
												   <tr>
													  <th scope="cols" colspan="2">구매 상품 정보</th>
													  <!-- <th scope="cols">구매 상품 정보</th> -->
													  <th scope="cols">상품 금액</th>
													  <th scope="cols">판매자</th>
													  <th></th>
												   </tr>
												</thead>
											 <%for(var i = 0; i < interest.length; i++) {%>
												<tr>
													<form action="/sellphone/delete_interest/<%= interest[i].interest_product %>/<%= interest[i].interest_id %>" method="post">
												   <td><a href="/read/<%= interest[i].interest_product %>"><img src="/electro/img/<%= interest[i].interest_img1 %>" height="100" width="100"></a></td> 
												   <td><a href="/read/<%= interest[i].interest_product %>"><%= interest[i].interest_title %></a></td>
												   <td><%= interest[i].interest_price %></td>
												   <td><%= interest[i].interest_customer %></td>
												   <td><button style="
													border: 2px solid transparent;
													height: 30px;
													padding: 0 20px;
													background-color: #ef233c;
													color: #FFF;
													text-transform: uppercase;
													font-weight: 700;
													border-radius: 40px;
													-webkit-transition: 0.2s all;
													transition: 0.2s all;" onclick="deletewishlist()">삭제</button></td>
													</form>
												</tr>
											 <% } %>
										  </table>
										  <%}else{%>
											<p>로그인을 먼저 해주세요!</p>
											<%}%>
										</div>
									</div>
								</div>
								<!-- /tab1  -->

								<!-- tab2  -->
								<div id="tab2" class="tab-pane fade in">
									<div class="row">
										<div class="col-md-9">
											<ul class="product-btns">
												<form action="/sellphone/question/<%=row.product_id%>" method="post">
												<li>
													<textarea class="input" type="text" name="question" placeholder=" 질문 작성" required></textarea>
												</li>
												<li>
													<button style="
													border: 2px solid transparent;
													height: 40px;
													padding: 0 30px;
													background-color: #ef233c;
													color: #FFF;
													text-transform: uppercase;
													font-weight: 700;
													border-radius: 40px;
													-webkit-transition: 0.2s all;
													transition: 0.2s all;"><i class="fa fa-pencil"></i> 질문작성</button>
												</li>
												</form>
											</ul>
											<hr style="border: solid 1px red;">
											<br>
											<ul>
												<% for(let i=0; i<question.length; i++) { %>
													<li style="font-size: 1.5em;"><%=question[i].question_creator%>	<%=question[i].question_content%>	<%=question[i].question_date%> </li>
													<% if(user!=null && (user[0].customer_id == row.product_saler || user[0].customer_kind == 1)) { %>
														<form action="/sellphone/answer/<%=row.product_id%>/<%=question[i].question_id%>" method="post">
															<li class="form-group">
																<input class="input" type="text" name="answer" placeholder=" 답변" required><button>달기</button></input>
															</li>
														</form>
													<% } for(let j=0; j< answer.length; j++) { %>
														<li>
															<% if(question[i].question_id == answer[j].answer_target) { %>
																<li style="font-size: 1.25em;">&nbsp&nbsp&nbsp&nbsp&nbsp<i class="fa fa-arrow-right"></i>&nbsp&nbsp<%=answer[j].answer_answer%> <%=answer[j].answer_content%> <%=answer[j].answer_date%></li>
															<% } %>
														</li>
													<% } %>
													
													<!-- <li><%=question[i].question_creator%></li>
													<li><%=question[i].question_date%></li> -->
													<hr style="border: solid 1px rgb(194, 194, 194);">
													<br>
												<% } %>
											</ul>
										</div>
									</div>
								</div>
								<!-- /tab2  -->

								<!-- tab3  -->
								<div id="tab3" class="tab-pane fade in">
									<div class="row">
										<!-- Rating -->
										<div class="col-md-3">
											<div id="rating">
											<% var sumreview = saler[0].customer_star5+saler[0].customer_star4+saler[0].customer_star3+saler[0].customer_star2+saler[0].customer_star1;
											   var review1;
											   var review2;
											   var review3;
											   var review4;
											   var review5;
											   if(sumreview == 0) {
												review1 = 0;
												review2 = 0;
												review3 = 0;
												review4 = 0;
												review5 = 0;
											   } else {
												review1 = (saler[0].customer_star1 *100)/sumreview;
												review2 = (saler[0].customer_star2 *100)/sumreview;
												review3 = (saler[0].customer_star3 *100)/sumreview;
												review4 = (saler[0].customer_star4 *100)/sumreview;
												review5 = (saler[0].customer_star5 *100)/sumreview;
											   }
											   var sumstar = (saler[0].customer_star5 * 5) + (saler[0].customer_star4 * 4) + (saler[0].customer_star3 * 3) + (saler[0].customer_star2 * 2) + (saler[0].customer_star1 * 1);
											   var average = sumstar/sumreview; %>
												<div class="rating-avg">
													<span><%=average.toFixed(1)%></span>
													<div class="rating-stars">
														<% if(average.toFixed(1)==5) { %>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
														<% } else if(average.toFixed(1)>=4.5) { %>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star-half-full" style="color:#D10024;"></i>
														<% } else if(average.toFixed(1)>=4.0) { %>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star-o"></i>
														<% } else if(average.toFixed(1)>=3.5) { %>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star-half-full" style="color:#D10024;"></i>
															<i class="fa fa-star-o"></i>
														<% } else if(average.toFixed(1)>=3.0) { %>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
														<% } else if(average.toFixed(1)>=2.5) { %>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star-half-full" style="color:#D10024;"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
														<% } else if(average.toFixed(1)>=2.0) { %>
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
														<% } else if(average.toFixed(1)>=1.5) { %>
															<i class="fa fa-star"></i>
															<i class="fa fa-star-half-empty" style="color:#D10024;"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
														<% } else if(average.toFixed(1)>=1.0) { %>
															<i class="fa fa-star"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
														<% } else if(average.toFixed(1)>=0.5) { %>
															<i class="fa fa-star-half-full" style="color:#D10024;"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
														<% } else if(average.toFixed(1)>=0.0) { %>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
															<i class="fa fa-star-o"></i>
														<% } %>
													</div>
												</div>
												<ul class="rating">
													<li>
														<div class="rating-stars">
															<i class="fa fa-star"></i>
															<i class="fa fa-star"></i>
